<main>
    <div class="container">
        <div class="col-md-7 col-lg-8">
            <hr class="my-4">
            <div>
                <label for="data" class="form-label">Result of your query: </label>
                <% if(Array.isArray(mydata)){%>
                    <table>
                        <tr><td>First Name</td><td>Last Name</td><td>Email</td><td>Phone</td></tr>

                    <%for(let a=0;a <mydata.length;a++){%>
                        <tr><td><%=mydata[a].firstName%></td><td><%=mydata[a].phone%></td><td><%=mydata[a].email%></td><td><%=mydata[a].mes%></td></tr>
                    <%}%>
                    </table>
                <%}else{%>
                <label for="data" class="form-label"> <%=mydata%>
                </label>
                <%}%>
            </div>
        </div>
    </div>
</main>


